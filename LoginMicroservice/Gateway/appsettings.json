{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "auth1": { "Address": "http://authenticationservice:5000" }
        }
      },
      "serverCluster": {
        "Destinations": {
          "server1": { "Address": "httpd://server:5001" }
        }
      }
    },
    "Routes": {
      "ping": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/ping"
        }
      },
      "auth": {
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "usersEf": {
        "ClusterId": "serverCluster",
        "Match": {
          "Path": "/api/users-ef/{**catch-all}"
        }
      }
    }
  }
}
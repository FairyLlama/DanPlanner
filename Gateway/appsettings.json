{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "auth1": { "Address": "http://auth_service:8080" }
        }
      },
      "serverCluster": {
        "Destinations": {
          "server1": { "Address": "http://danplanner_server:8080" }
        }
      }
    },
    "Routes": {
      "ping": {
        "ClusterId": "authCluster",
        "Match": { "Path": "/ping" }
      },
      "auth": {
        "ClusterId": "authCluster",
        "Match": { "Path": "/auth/{**catch-all}" }
      },
      "usersEf": {
        "ClusterId": "serverCluster",
        "Match": { "Path": "/api/users-ef/{**catch-all}" }
      }
    }
  }
}